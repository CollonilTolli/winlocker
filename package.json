{
  "name": "CollonilTolliWinLock",
  "version": "1.0.0",
  "description": "Шуточный винлокер для друга",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-packager . CollonilTolliWinLock --platform=win32 --arch=x64 --out=dist --overwrite",
    "installer": "cmd /c \"\"C:\\Program Files (x86)\\NSIS\\Bin\\makensis.exe\" installer.nsi\""
  },
  "keywords": [
    "electron",
    "CollonilTolliWinLock",
    "prank"
  ],
  "author": "Collonil Tolli",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-packager": "^17.1.2"
  },
  "dependencies": {
    "auto-launch": "^5.0.5"
  },
  "build": {
    "appId": "com.CollonilTolliWinLock.app",
    "productName": "CollonilTolliWinLock",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": {
        "target": "nsis",
        "arch": ["x64"]
      },
      "publisherName": "CollonilTolliWinLock"
    },
    "forceCodeSigning": false,
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "CollonilTolliWinLock",
      "installerIcon": "background.png",
      "uninstallerIcon": "background.png",
      "artifactName": "CollonilTolliWinLock-Setup.exe"
    }
  }
}